-- Initialization that requires path substitution.
create database contrib_ora_dblink sql mode opentenbase_ora;
\c contrib_ora_dblink
CREATE FUNCTION putenv(text)
   RETURNS void
   AS '@libdir@/regress@DLSUFFIX@', 'regress_putenv'
   LANGUAGE C STRICT;
CREATE FUNCTION wait_pid(int)
   RETURNS void
   AS '@libdir@/regress@DLSUFFIX@', 'wait_pid'
   LANGUAGE C STRICT;
CREATE FUNCTION set_pgservicefile(text) RETURNS void LANGUAGE SQL
    AS $$SELECT putenv('PGSERVICEFILE=@abs_srcdir@/' || $1)$$;
