View definition:
 SELECT X.TBLOWNERNAME AS OWNER,
    X.CSTRSCHEMA AS CONSTRAINT_SCHEMA,
    X.CSTRNAME AS CONSTRAINT_NAME,
    X.TBLSCHEMA AS TABLE_SCHEMA,
    X.TBLNAME AS TABLE_NAME,
    X.COLNAME AS COLUMN_NAME,
    X.COLPOSITION AS "POSITION"
   FROM ( SELECT DISTINCT R.OID,
            NR.NSPNAME,
            R.RELNAME,
            R.RELOWNER,
            A.ATTNAME,
            NC.NSPNAME,
            C.CONNAME,
            A.ATTNUM,
            U.ROLNAME
           FROM PG_NAMESPACE NR,
            PG_CLASS R,
            PG_ATTRIBUTE A,
            PG_DEPEND D,
            PG_NAMESPACE NC,
            PG_CONSTRAINT C,
            PG_AUTHID U
          WHERE NR.OID = R.RELNAMESPACE AND R.OID = A.ATTRELID AND U.OID = R.RELOWNER AND D.REFCLASSID = '"pg_class"'::REGCLASS::OID AND D.CLASSID = '"pg_constraint"'::REGCLASS::OID AND D.REFOBJID = R.OID AND D.REFOBJSUBID = A.ATTNUM AND D.OBJID = C.OID AND C.CONNAMESPACE = NC.OID AND C.CONTYPE = 'c'::"CHAR" AND (R.RELKIND = ANY (ARRAY['r'::"CHAR", 'p'::"CHAR"])) AND NOT A.ATTISDROPPED
        UNION ALL
         SELECT R.OID,
            NR.NSPNAME,
            R.RELNAME,
            R.RELOWNER,
            A.ATTNAME,
            NC.NSPNAME,
            C.CONNAME,
            A.ATTNUM,
            U.ROLNAME
           FROM PG_NAMESPACE NR,
            PG_CLASS R,
            PG_ATTRIBUTE A,
            PG_NAMESPACE NC,
            PG_CONSTRAINT C,
            PG_AUTHID U
          WHERE NR.OID = R.RELNAMESPACE AND R.OID = A.ATTRELID AND R.RELOWNER = U.OID AND NC.OID = C.CONNAMESPACE AND R.OID = C.CONRELID AND (A.ATTNUM = ANY (ARRAY[C.CONKEY])) AND NOT A.ATTISDROPPED AND (C.CONTYPE = ANY (ARRAY['p'::"CHAR", 'u'::"CHAR", 'f'::"CHAR"])) AND (R.RELKIND = ANY (ARRAY['r'::"CHAR", 'p'::"CHAR"]))
        UNION ALL
         SELECT R.OID,
            NR.NSPNAME,
            R.RELNAME,
            R.RELOWNER,
            A.ATTNAME,
            NR.NSPNAME,
            ((((NR.OID::TEXT || '_'::TEXT) || R.OID::TEXT) || '_'::TEXT) || A.ATTNUM::TEXT) || '_not_null'::TEXT,
            A.ATTNUM,
            U.ROLNAME
           FROM PG_NAMESPACE NR,
            PG_CLASS R,
            PG_ATTRIBUTE A,
            PG_AUTHID U
          WHERE NR.OID = R.RELNAMESPACE AND R.OID = A.ATTRELID AND R.RELOWNER = U.OID AND A.ATTNOTNULL AND A.ATTNUM > 0 AND NOT A.ATTISDROPPED AND (R.RELKIND = ANY (ARRAY['r'::"CHAR", 'p'::"CHAR"])) AND NOT PG_IS_OTHER_TEMP_SCHEMA(NR.OID)) X(TBLOID, TBLSCHEMA, TBLNAME, TBLOWNEROID, COLNAME, CSTRSCHEMA, CSTRNAME, COLPOSITION, TBLOWNERNAME)
  WHERE PG_HAS_ROLE(X.TBLOWNEROID, 'USAGE'::TEXT) OR HAS_TABLE_PRIVILEGE(X.TBLOID, 'SELECT, INSERT, UPDATE, DELETE, TRUNCATE, REFERENCES, TRIGGER'::TEXT) OR HAS_ANY_COLUMN_PRIVILEGE(X.TBLOID, 'SELECT, INSERT, UPDATE, REFERENCES'::TEXT);

