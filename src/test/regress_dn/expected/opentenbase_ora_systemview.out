\d+ internal_constraints;
                         View "pg_catalog.INTERNAL_CONSTRAINTS"
       Column        |  Type   | Collation | Nullable | Default | Storage  | Description 
---------------------+---------+-----------+----------+---------+----------+-------------
 OWNER               | NAME    |           |          |         | plain    | 
 CONSTRAINT_SCHEMA   | NAME    |           |          |         | plain    | 
 CONSTRAINT_NAME     | NAME    |           |          |         | plain    | 
 CONSTRAINT_TYPE     | TEXT    |           |          |         | extended | 
 TABLE_SCHEMA        | NAME    |           |          |         | plain    | 
 TABLE_NAME          | NAME    |           |          |         | plain    | 
 R_OWNER             | NAME    |           |          |         | plain    | 
 R_CONSTRAINT_SCHEMA | NAME    |           |          |         | plain    | 
 R_CONSTRAINT_NAME   | NAME    |           |          |         | plain    | 
 DELETE_RULE         | TEXT    |           |          |         | extended | 
 IS_DEFERRABLE       | BOOLEAN |           |          |         | plain    | 
 DEFERRED            | BOOLEAN |           |          |         | plain    | 
 REL_OWNER           | OID     |           |          |         | plain    | 
 REL_OID             | OID     |           |          |         | plain    | 
View definition:
 SELECT U.ROLNAME AS OWNER,
    CONNSP.NSPNAME AS CONSTRAINT_SCHEMA,
    CON.CONNAME AS CONSTRAINT_NAME,
        CASE CON.CONTYPE
            WHEN 'c'::"CHAR" THEN 'C'::TEXT
            WHEN 'f'::"CHAR" THEN 'R'::TEXT
            WHEN 'p'::"CHAR" THEN 'P'::TEXT
            WHEN 'u'::"CHAR" THEN 'U'::TEXT
            ELSE NULL::TEXT
        END AS CONSTRAINT_TYPE,
    RNSP.NSPNAME AS TABLE_SCHEMA,
    R.RELNAME AS TABLE_NAME,
        CASE CON.CONTYPE
            WHEN 'f'::"CHAR" THEN FU.ROLNAME
            ELSE NULL::NAME
        END AS R_OWNER,
        CASE CON.CONTYPE
            WHEN 'f'::"CHAR" THEN NPKC.NSPNAME
            ELSE NULL::NAME
        END AS R_CONSTRAINT_SCHEMA,
        CASE CON.CONTYPE
            WHEN 'f'::"CHAR" THEN PKC.CONNAME
            ELSE NULL::NAME
        END AS R_CONSTRAINT_NAME,
        CASE CON.CONFDELTYPE
            WHEN 'c'::"CHAR" THEN 'CASCADE'::TEXT
            WHEN 'n'::"CHAR" THEN 'SET NULL'::TEXT
            WHEN 'd'::"CHAR" THEN 'SET DEFAULT'::TEXT
            WHEN 'r'::"CHAR" THEN 'RESTRICT'::TEXT
            WHEN 'a'::"CHAR" THEN 'NO ACTION'::TEXT
            ELSE NULL::TEXT
        END AS DELETE_RULE,
    CON.CONDEFERRABLE AS IS_DEFERRABLE,
    CON.CONDEFERRED AS DEFERRED,
    R.RELOWNER AS REL_OWNER,
    R.OID AS REL_OID
   FROM PG_CONSTRAINT CON
     LEFT JOIN PG_NAMESPACE CONNSP ON CONNSP.OID = CON.CONNAMESPACE
     LEFT JOIN PG_CLASS R ON (R.RELKIND = ANY (ARRAY['r'::"CHAR", 'p'::"CHAR"])) AND CON.CONRELID = R.OID
     LEFT JOIN PG_AUTHID U ON R.RELOWNER = U.OID
     LEFT JOIN PG_NAMESPACE RNSP ON RNSP.OID = R.RELNAMESPACE
     LEFT JOIN (PG_CLASS FR
     LEFT JOIN PG_AUTHID FU ON FU.OID = FR.RELOWNER) ON CON.CONFRELID = FR.OID AND (CON.CONTYPE <> ALL (ARRAY['t'::"CHAR", 'x'::"CHAR"]))
     LEFT JOIN PG_DEPEND D1 ON D1.OBJID = CON.OID AND D1.CLASSID = '"pg_constraint"'::REGCLASS::OID AND D1.REFCLASSID = '"pg_class"'::REGCLASS::OID AND D1.REFOBJSUBID = 0
     LEFT JOIN PG_DEPEND D2 ON D2.REFCLASSID = '"pg_constraint"'::REGCLASS::OID AND D2.CLASSID = '"pg_class"'::REGCLASS::OID AND D2.OBJID = D1.REFOBJID AND D2.OBJSUBID = 0 AND D2.DEPTYPE = 'i'::"CHAR"
     LEFT JOIN PG_CONSTRAINT PKC ON PKC.OID = D2.REFOBJID AND (PKC.CONTYPE = ANY (ARRAY['p'::"CHAR", 'u'::"CHAR"])) AND PKC.CONRELID = CON.CONFRELID
     LEFT JOIN PG_NAMESPACE NPKC ON PKC.CONNAMESPACE = NPKC.OID
UNION ALL
 SELECT U.ROLNAME AS OWNER,
    NR.NSPNAME AS CONSTRAINT_SCHEMA,
    ((((NR.OID::TEXT || '_'::TEXT) || R.OID::TEXT) || '_'::TEXT) || A.ATTNUM::TEXT) || '_not_null'::TEXT AS CONSTRAINT_NAME,
    'C'::TEXT AS CONSTRAINT_TYPE,
    NR.NSPNAME AS TABLE_SCHEMA,
    R.RELNAME AS TABLE_NAME,
    NULL::NAME AS R_OWNER,
    NULL::NAME AS R_CONSTRAINT_SCHEMA,
    NULL::NAME AS R_CONSTRAINT_NAME,
    NULL::TEXT AS DELETE_RULE,
    false AS IS_DEFERRABLE,
    false AS DEFERRED,
    R.RELOWNER AS REL_OWNER,
    R.OID AS REL_OID
   FROM PG_NAMESPACE NR,
    PG_CLASS R,
    PG_ATTRIBUTE A,
    PG_AUTHID U
  WHERE NR.OID = R.RELNAMESPACE AND R.OID = A.ATTRELID AND R.RELOWNER = U.OID AND A.ATTNOTNULL AND A.ATTNUM > 0 AND NOT A.ATTISDROPPED AND (R.RELKIND = ANY (ARRAY['r'::"CHAR", 'p'::"CHAR"])) AND NOT PG_IS_OTHER_TEMP_SCHEMA(NR.OID);

