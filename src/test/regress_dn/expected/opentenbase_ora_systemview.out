View definition:
 SELECT U.ROLNAME AS OWNER,
    NC.NSPNAME AS TABLE_SCHEMA,
    C.RELNAME AS TABLE_NAME,
    A.ATTNAME AS COLUMN_NAME,
        CASE
            WHEN T.TYPTYPE = 'd'::"CHAR" THEN
            CASE
                WHEN BT.TYPELEM <> 0::OID AND BT.TYPLEN = '-1'::INTEGER THEN 'ARRAY'::TEXT
                WHEN NBT.NSPNAME = 'pg_catalog'::NAME THEN FORMAT_TYPE(T.TYPBASETYPE, NULL::INTEGER)
                ELSE 'USER-DEFINED'::TEXT
            END
            ELSE
            CASE
                WHEN T.TYPELEM <> 0::OID AND T.TYPLEN = '-1'::INTEGER THEN 'ARRAY'::TEXT
                WHEN NT.NSPNAME = 'pg_catalog'::NAME THEN FORMAT_TYPE(A.ATTTYPID, NULL::INTEGER)
                ELSE 'USER-DEFINED'::TEXT
            END
        END AS DATA_TYPE,
    TU.ROLNAME AS DATA_TYPE_OWNER,
    COALESCE(_PG_CHAR_MAX_LENGTH(_PG_TRUETYPID(A.*, T.*), _PG_TRUETYPMOD(A.*, T.*)), _PG_NUMERIC_PRECISION(_PG_TRUETYPID(A.*, T.*), _PG_TRUETYPMOD(A.*, T.*))) AS DATA_LENGTH,
    _PG_NUMERIC_PRECISION(_PG_TRUETYPID(A.*, T.*), _PG_TRUETYPMOD(A.*, T.*)) AS DATA_PRECISION,
    _PG_NUMERIC_SCALE(_PG_TRUETYPID(A.*, T.*), _PG_TRUETYPMOD(A.*, T.*)) AS DATA_SCALE,
        CASE
            WHEN A.ATTNOTNULL OR T.TYPTYPE = 'd'::"CHAR" AND T.TYPNOTNULL THEN 'NO'::TEXT
            ELSE 'YES'::TEXT
        END AS NULLABLE,
    A.ATTNUM AS COLUMN_ID,
    PG_GET_EXPR(AD.ADBIN, AD.ADRELID) AS DATA_DEFAULT,
        CASE
            WHEN S.STADISTINCT >= 0::DOUBLE PRECISION THEN S.STADISTINCT::DOUBLE PRECISION
            WHEN S.STADISTINCT < 0::DOUBLE PRECISION THEN S.STADISTINCT * '-1'::INTEGER::DOUBLE PRECISION * C.RELTUPLES
            ELSE NULL::DOUBLE PRECISION
        END::NUMERIC(38,0) AS NUM_DISTINCT,
    NULL::TEXT AS LOW_VALUE,
    NULL::TEXT AS HIGH_VALUE,
    NULL::TEXT AS DENSITY,
    C.RELTUPLES * S.STANULLFRAC AS NUM_NULLS,
        CASE
            WHEN S.STAKIND1 = 2 THEN ARRAY_LENGTH(S.STAVALUES1, 1)
            WHEN S.STAKIND2 = 2 THEN ARRAY_LENGTH(S.STAVALUES2, 1)
            WHEN S.STAKIND3 = 2 THEN ARRAY_LENGTH(S.STAVALUES3, 1)
            WHEN S.STAKIND4 = 2 THEN ARRAY_LENGTH(S.STAVALUES4, 1)
            WHEN S.STAKIND5 = 2 THEN ARRAY_LENGTH(S.STAVALUES5, 1)
            ELSE NULL::INTEGER
        END AS NUM_BUCKETS,
    NULL::TEXT AS LAST_ANALYZED,
    NULL::TEXT AS SAMPLE_SIZE,
        CASE
            WHEN (( SELECT COUNT(*) AS COUNT
               FROM PG_TYPE T_1
              WHERE T_1.OID = A.ATTTYPID AND (T_1.TYPNAME = ANY (ARRAY['char'::NAME, 'varchar'::NAME, 'text'::NAME, 'varchar2'::NAME, 'nvarchar2'::NAME, '_varchar2'::NAME, '_nvarchar2'::NAME, '_char'::NAME, '_varchar'::NAME, '_text'::NAME])))) > 0::NUMERIC THEN ( SELECT PG_SETTINGS.SETTING
               FROM PG_SETTINGS
              WHERE PG_SETTINGS.NAME = 'server_encoding'::TEXT)
            ELSE NULL::TEXT
        END AS CHARACTER_SET_NAME,
        CASE
            WHEN (( SELECT COUNT(*) AS COUNT
               FROM PG_TYPE T_1
              WHERE T_1.OID = A.ATTTYPID AND (T_1.TYPNAME = ANY (ARRAY['char'::NAME, 'varchar'::NAME, 'varchar2'::NAME, 'nvarchar2'::NAME])))) > 0::NUMERIC THEN A.ATTTYPMOD - 4
            ELSE NULL::INTEGER
        END AS CHAR_COL_DECL_LENGTH,
    'NO'::TEXT AS GLOBAL_STATS,
    'YES'::TEXT AS USER_STATS,
    S.STAWIDTH AS AVG_COL_LEN,
        CASE
            WHEN (( SELECT COUNT(*) AS COUNT
               FROM PG_TYPE T_1
              WHERE T_1.OID = A.ATTTYPID AND (T_1.TYPNAME = ANY (ARRAY['char'::NAME, 'varchar'::NAME, 'varchar2'::NAME, 'nvarchar2'::NAME])))) > 0::NUMERIC THEN A.ATTTYPMOD - 4
            ELSE NULL::INTEGER
        END AS CHAR_LENGTH,
    'B'::TEXT AS CHAR_USED,
    NULL::TEXT AS V80_FMT_IMAGE,
    NULL::TEXT AS DATA_UPGRADED,
        CASE
            WHEN S.STAKIND1 = 2 THEN 'HEIGHT BALANCED'::TEXT
            WHEN S.STAKIND2 = 2 THEN 'HEIGHT BALANCED'::TEXT
            WHEN S.STAKIND3 = 2 THEN 'HEIGHT BALANCED'::TEXT
            WHEN S.STAKIND4 = 2 THEN 'HEIGHT BALANCED'::TEXT
            WHEN S.STAKIND5 = 2 THEN 'HEIGHT BALANCED'::TEXT
            ELSE
            CASE
                WHEN S.STAKIND1 = 2 THEN 'FREQUENCY'::TEXT
                WHEN S.STAKIND2 = 2 THEN 'FREQUENCY'::TEXT
                WHEN S.STAKIND3 = 2 THEN 'FREQUENCY'::TEXT
                WHEN S.STAKIND4 = 2 THEN 'FREQUENCY'::TEXT
                WHEN S.STAKIND5 = 2 THEN 'FREQUENCY'::TEXT
                ELSE 'NONE'::TEXT
            END
        END AS HISTOGRAM,
    C.RELOWNER AS REL_OWNER,
    C.OID AS REL_OID,
    A.ATTNUM AS REL_ATTNUM
   FROM PG_ATTRIBUTE A
     LEFT JOIN PG_ATTRDEF AD ON A.ATTRELID = AD.ADRELID AND A.ATTNUM = AD.ADNUM
     JOIN (PG_CLASS C
     JOIN PG_NAMESPACE NC ON C.RELNAMESPACE = NC.OID
     JOIN PG_AUTHID U ON U.OID = C.RELOWNER) ON A.ATTRELID = C.OID
     JOIN (PG_TYPE T
     JOIN PG_NAMESPACE NT ON T.TYPNAMESPACE = NT.OID
     LEFT JOIN PG_AUTHID TU ON TU.OID = T.TYPOWNER) ON A.ATTTYPID = T.OID
     LEFT JOIN (PG_TYPE BT
     JOIN PG_NAMESPACE NBT ON BT.TYPNAMESPACE = NBT.OID) ON T.TYPTYPE = 'd'::"CHAR" AND T.TYPBASETYPE = BT.OID
     LEFT JOIN (PG_COLLATION CO
     JOIN PG_NAMESPACE NCO ON CO.COLLNAMESPACE = NCO.OID) ON A.ATTCOLLATION = CO.OID AND (NCO.NSPNAME <> 'pg_catalog'::NAME OR CO.COLLNAME <> 'default'::NAME)
     LEFT JOIN (PG_DEPEND DEP
     JOIN PG_SEQUENCE SEQ ON DEP.CLASSID = '"pg_class"'::REGCLASS::OID AND DEP.OBJID = SEQ.SEQRELID AND DEP.DEPTYPE = 'i'::"CHAR") ON DEP.REFCLASSID = '"pg_class"'::REGCLASS::OID AND DEP.REFOBJID = C.OID AND DEP.REFOBJSUBID = A.ATTNUM
     LEFT JOIN PG_STATISTIC S ON S.STARELID = C.OID AND S.STAATTNUM = A.ATTNUM
  WHERE NOT PG_IS_OTHER_TEMP_SCHEMA(NC.OID) AND A.ATTNUM > 0 AND NOT A.ATTISDROPPED AND (C.RELKIND = ANY (ARRAY['r'::"CHAR", 'v'::"CHAR", 'f'::"CHAR", 'p'::"CHAR"]));

