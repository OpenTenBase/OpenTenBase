View definition:
 SELECT ( SELECT PG_AUTHID.ROLNAME
           FROM PG_AUTHID
          WHERE PG_AUTHID.OID = IDX_I.RELOWNER) AS OWNER,
    IDX_I.RELNAME AS INDEX_NAME,
    ( SELECT PG_NAMESPACE.NSPNAME
           FROM PG_NAMESPACE
          WHERE PG_NAMESPACE.OID = IDX_I.RELNAMESPACE) AS INDEX_NSP,
    ( SELECT PG_AM.AMNAME
           FROM PG_AM
          WHERE PG_AM.OID = IDX_I.RELAM) AS INDEX_TYPE,
    ( SELECT PG_AUTHID.ROLNAME
           FROM PG_AUTHID
          WHERE PG_AUTHID.OID = IDX_R.RELOWNER) AS TABLE_OWNER,
    IDX_R.RELNAME AS TABLE_NAME,
    ( SELECT PG_NAMESPACE.NSPNAME
           FROM PG_NAMESPACE
          WHERE PG_NAMESPACE.OID = IDX_R.RELNAMESPACE) AS TABLE_NSP,
    'TABLE'::TEXT AS TABLE_TYPE,
        CASE
            WHEN I.INDISUNIQUE THEN 'UNIQUE'::TEXT
            ELSE 'NONUNIQUE'::TEXT
        END AS UNIQUENESS,
    'DISABLED'::TEXT AS COMPRESSION,
    NULL::TEXT AS PREFIX_LENGTH,
    ( SELECT PG_TABLESPACE.SPCNAME
           FROM PG_TABLESPACE
          WHERE PG_TABLESPACE.OID = IDX_I.RELTABLESPACE) AS TABLESPACE_NAME,
    NULL::TEXT AS INI_TRANS,
    NULL::TEXT AS MAX_TRANS,
    NULL::TEXT AS INITIAL_EXTENT,
    NULL::TEXT AS NEXT_EXTENT,
    NULL::TEXT AS MIN_EXTENTS,
    NULL::TEXT AS MAX_EXTENTS,
    NULL::TEXT AS PCT_INCREASE,
    10 AS PCT_THRESHOLD,
    NULL::TEXT AS INCLUDE_COLUMN,
    NULL::TEXT AS FREELISTS,
    NULL::TEXT AS FREELIST_GROUPS,
    NULL::TEXT AS PCT_FREE,
    'NO'::TEXT AS LOGGING,
    0 AS BLEVEL,
    0 AS LEAF_BLOCKS,
    0 AS DISTINCT_KEYS,
        CASE
            WHEN I.INDISUNIQUE THEN 1
            ELSE 0
        END AS AVG_LEAF_BLOCKS_PER_KEY,
    0 AS AVG_DATA_BLOCKS_PER_KEY,
    0 AS CLUSTERING_FACTOR,
        CASE
            WHEN I.INDISVALID THEN 'VALID'::TEXT
            ELSE 'UNUSABLE'::TEXT
        END AS STATUS,
    IDX_R.RELTUPLES AS NUM_ROWS,
    0 AS SAMPLE_SIZE,
    NULL::TEXT AS LAST_ANALYZED,
    NULL::TEXT AS DEGREE,
    NULL::TEXT AS INSTANCES,
        CASE
            WHEN IDX_I.RELKIND = 'I'::"CHAR" THEN 'YES'::TEXT
            ELSE 'NO'::TEXT
        END AS PARTITIONED,
        CASE
            WHEN IDX_I.RELKIND = 't'::"CHAR" THEN 'Y'::TEXT
            ELSE 'N'::TEXT
        END AS TEMPORARY,
    'N'::TEXT AS GENERATED,
    'N'::TEXT AS SECONDARY,
    'DEFAULT'::TEXT AS BUFFER_POOL,
    'NO'::TEXT AS USER_STATS,
    NULL::TEXT AS DURATION,
    0 AS PCT_DIRECT_ACCESS,
    NULL::TEXT AS ITYP_OWNER,
    NULL::TEXT AS ITYP_NAME,
    NULL::TEXT AS PARAMETERS,
    'NO'::TEXT AS GLOBAL_STATS,
    NULL::TEXT AS DOMIDX_STATUS,
    NULL::TEXT AS DOMIDX_OPSTATUS,
        CASE
            WHEN I.INDEXPRS IS NOT NULL THEN 'ENABLED'::TEXT
            ELSE NULL::TEXT
        END AS FUNCIDX_STATUS,
    'NO'::TEXT AS JOIN_INDEX,
    NULL::TEXT AS IOT_REDUNDANT_PKEY_ELIM,
    'NO'::TEXT AS DROPPED,
    IDX_R.OID AS RELID
   FROM PG_INDEX I
     JOIN PG_CLASS IDX_I ON I.INDEXRELID = IDX_I.OID
     JOIN PG_CLASS IDX_R ON I.INDRELID = IDX_R.OID
  WHERE IDX_R.RELKIND = ANY (ARRAY['r'::"CHAR", 'v'::"CHAR", 'f'::"CHAR", 'p'::"CHAR"]);

