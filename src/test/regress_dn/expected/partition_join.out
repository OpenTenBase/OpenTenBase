SELECT t1.a, t1.c, t2.b, t2.c FROM prt1 t1, prt2 t2 WHERE t1.a = t2.b AND t1.a < 450 AND t2.b > 250 AND t1.b = 0 ORDER BY t1.a, t2.b;
                        QUERY PLAN                         
-----------------------------------------------------------
 Sort
   Sort Key: t1.a
   ->  Hash Join
         Hash Cond: (t2.b = t1.a)
         ->  PartIterator
               ->  Seq Scan on prt2 t2
                     Filter: (b > 250)
         ->  Hash
               ->  PartIterator
                     ->  Seq Scan on prt1 t1
                           Filter: ((a < 450) AND (b = 0))
(11 rows)

